<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="Locat" table="bicycle_locat">
		<id name="id" length="11" >
		<generator class="native" />
		</id>
		<property name="geo" length="20" not-null="true" />
		<property name="time" length="40" not-null="true" />
		<many-to-one name="userCommonInfo" class="com.wxxiaomi.ebs.bean.UserCommonInfo"
		access="field"
			column="userid" unique="true" lazy="false" />
	</class>
</hibernate-mapping>
