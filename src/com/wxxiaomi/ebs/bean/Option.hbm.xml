<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="Option" table="bicycle_option">
		<id name="id" length="11" >
		<generator class="native" />
		</id>
		<property name="user_id"/>
		<property name="obj_type"/>
		<property name="obj_id"/>
		<property name="parent_type"/>
		<property name="parent_id"/>
		<property name="create_time" type="timestamp" length="19"/> 
	</class>
</hibernate-mapping>
