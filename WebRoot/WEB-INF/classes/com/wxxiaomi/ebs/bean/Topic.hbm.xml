<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="Topic" table="bicycle_topic">
		<id name="id" length="11">
			<generator class="native" />
		</id>
		<many-to-one unique="true" column="userid" access="field"
			name="userCommonInfo" lazy="false" />
		
		<property name="time" column="time" type="timestamp" length="19"/> 
		
		<property name="content" not-null="true" />
		<property name="pics" />
		<property name="hot" />
		<property name="ccount"/>
		<property name="locat" />
		<property name="locat_tag"/>
		<property name="title"  />
	</class>
</hibernate-mapping>
