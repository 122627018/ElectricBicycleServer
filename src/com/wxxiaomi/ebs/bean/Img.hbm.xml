<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="Img" table="bicycle_img">
		<id name="id" length="11">
			<generator class="native" />
		</id>
		<property name="belongId" not-null="true" />
		<property name="path" not-null="true" />
		<property name="description" not-null="true" />
		<property name="bigPath" not-null="true" />
		<property name="type" not-null="true" />
		<property name="time" not-null="true" />
	</class>
</hibernate-mapping>
