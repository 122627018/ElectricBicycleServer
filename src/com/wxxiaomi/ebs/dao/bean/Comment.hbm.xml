<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.dao.bean">
 <class name="Comment" table="bicycle_comment">
  <id length="11" name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="topic_id"/>
  <property generated="never" lazy="false" name="content"/>
  <property generated="never" lazy="false" name="from_uid"/>
  <property generated="never" lazy="false" name="from_nick"/>
  <property generated="never" lazy="false" name="from_head"/>
  <property generated="never" lazy="false" name="to_uid"/>
  <property generated="never" lazy="false" name="to_unick"/>
 </class>
</hibernate-mapping>
