<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="OptionLogs" table="bicycle_opt_logs">
		<id name="id" length="11">
			<generator class="native" />
		</id>
		<property name="obj_type" not-null="true" />
		<property name="obj_id" not-null="true" />
		<property name="userid" not-null="true" />
		<property name="create_time" column="create_time" type="timestamp" length="19"/> 
		<property name="content"/>
		<property name="pictures" />
		<property name="title" />
		<property name="foor_note"/>
		<property name="locat" />
		<property name="locat_tag"/>
		<property name="start"  />
	</class>
</hibernate-mapping>
