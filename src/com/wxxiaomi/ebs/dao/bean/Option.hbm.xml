<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.dao.bean">
 <class name="Option" table="bicycle_option">
  <id length="11" name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="user_id"/>
  <property generated="never" lazy="false" name="obj_type"/>
  <property generated="never" lazy="false" name="obj_id"/>
  <property generated="never" lazy="false" name="parent_type"/>
  <property generated="never" lazy="false" name="parent_id"/>
  <property generated="never" lazy="false" name="attch"/>
  <property generated="never" lazy="false" name="type"/>
  <property generated="never" lazy="false" length="19"
   name="create_time" type="timestamp"/>
 </class>
</hibernate-mapping>
