<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wxxiaomi.ebs.bean">
	<class name="Comment" table="bicycle_comment">
		<id name="id" length="11" >
		<generator class="native" />
		</id>
		<property name="topic_id"/>
		<property name="content"/>
		<property name="from_uid"/>
		<property name="from_nick"/>
		<property name="from_head"/>
		<property name="to_uid"/>
		<property name="to_unick" />

	</class>
</hibernate-mapping>
